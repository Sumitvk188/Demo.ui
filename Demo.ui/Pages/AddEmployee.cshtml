@page
@model Demo.ui.Pages.AddEmployeeModel
@{

}
<form method="post">
<div class="form-group">
     <label asp-for="request.Name">Employee Name:</label>
     <input asp-for="request.Name" type="text" class="form-control" id="Name" />
 </div>
 <div class="form-group">
     <label for="Email">Employee Email:</label>
     <input asp-for="request.Email" type="email" class="form-control" id="Email" />
 </div>
 <button type="submit" class="btn btn-primary">Create Employee</button>
</form>
