@page
@model Demo.ui.Pages.UpdateEmployeeModel
@{
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">

            <input asp-for="emp.Id" type="hidden" class="form-control" id="Id" />
        </div>
        <div class="form-group">
            <label asp-for="emp.Name">Employee Name:</label>
            <input asp-for="emp.Name" type="text" class="form-control" id="Name" />
        </div>
        <div class="form-group">
            <label asp-for="emp.Email">Employee Email:</label>
            <input asp-for="emp.Email" type="email" class="form-control" id="Email" />
        </div>
        <button type="submit" class="btn btn-primary">Edit Employee</button>


    </form>
}
